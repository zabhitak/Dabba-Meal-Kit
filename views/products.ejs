            </div>
            </div>
            <div class="row">
                <% products.forEach(eachProduct => { %> 
                    <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12">
                        <div class="all-meal">
                            <div class="top">
                                <a href="productDetail-<%=eachProduct.id%>"><div class="bg-gradient"></div></a>
                                <div class="top-img">
                                    <img src="<%=eachProduct.images[0]%>" alt="">
                                </div>
                                <div class="logo-img">
                                    <img src="<%=eachProduct.user.avatar%>" alt="">
                                </div>
                                <div class="top-text">
                                    <div class="heading"><h4><a href="productDetail-<%=eachProduct.id%>"> <%= eachProduct.title %> </a></h4></div>
                                    <div class="sub-heading">
                                        <h5><a href="restaurant_detail.html"><%= eachProduct.category %></a></h5>
                                        <p><%= eachProduct.price %></p>
                                    </div>
                                </div>
                            </div>
                            <div class="bottom">
                            <div class="bottom-text">
                            <div class="delivery"><i class="fas fa-shopping-cart"></i>Delivery Free : <%= eachProduct.deliveryCharge %> </div>
                            <div class="time"><i class="far fa-clock"></i>Delivery Time : <%= eachProduct.deliveryTime %></div>
                            <div class="star">
                            <span> Comments </span>
                            <div class="comments"><a href="#"><i class="fas fa-comment-alt"></i> <%= eachProduct.comments.length %> </a></div>
                            </div>
                            </div>
                            </div>
                        </div>
